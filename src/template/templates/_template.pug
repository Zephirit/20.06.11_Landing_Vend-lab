block variables

doctype html
html(lang="ru-Ru")
	head
		meta(charset="utf-8")
		title #{title}
		meta(content="SM-group" name="author")
		meta(content="description" name="description")
		meta(content="prepros" name="keywords")
		meta(content="width=device-width, initial-scale=1" name="viewport")
		meta(content="ie=edge" http-equiv="x-ua-compatible")

		//- favicon block start
		link(rel="shortcut icon" href="./favicon.ico" type="image/ico")
		//- favicon block end

		//- link(rel="manifest" href="./manifest.json")
		<!----------------------------------------------------------------------------->
		<!-- Библиотеки и плагины CSS-->
		link(rel='stylesheet' href='./templates/default/plugins/bootstrap-4.5.0/css/bootstrap-reboot.min.css')
		link(rel='stylesheet' href='./templates/default/plugins/slick/slick.css')
		link(rel='stylesheet' href='./templates/default/plugins/animate.min.css')
		link(rel='stylesheet' href='./templates/default/plugins/hover-min.css')
		<!----------------------------------------------------------------------------->
		<!-- Основной файл стилей -->
		link(rel='stylesheet' href='./templates/default/template_styles.css')
		


	body
		include ../common/_mixins
		.wrapper
			block content
		<!----------------------------------------------------------------------------->
		<!-- Библиотеки и плагины JS-->
		script(src="./templates/default/plugins/jquery-3.5.1.min.js")
		script(src="./templates/default/plugins/slick/slick.min.js")
		<!----------------------------------------------------------------------------->
		<!-- Основной файл скриптов -->
		script(src="./templates/default/script.js")

		<!--[if lt IE 9]>
		script(src="http://cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.2/html5shiv.min.js")
		<![endif]-->
